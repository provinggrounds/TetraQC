    RowBox[{"{", 
     RowBox[{"x1_", ",", "x2_", ",", "x3_"}], "}"}], "]"}], ":=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Normalize", "[", 
      RowBox[{"Cross", "[", 
       RowBox[{
        RowBox[{"x3", "-", "x1"}], ",", 
        RowBox[{"x2", "-", "x1"}]}], "]"}], "]"}], ",", "x2"}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
