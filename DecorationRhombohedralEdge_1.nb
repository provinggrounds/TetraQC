      RowBox[{"n2_", ",", "p2_"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"n3_", ",", "p3_"}], "}"}]}], "]"}], ":=", 
   RowBox[{
    RowBox[{"1", "/", 
