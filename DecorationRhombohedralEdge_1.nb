      RowBox[{
       RowBox[{"(", 
        RowBox[{"p2", ".", "n2"}], ")"}], 
       RowBox[{"Cross", "[", 
        RowBox[{"n3", ",", "n1"}], "]"}]}], "+", 
      RowBox[{
       RowBox[{"(", 
